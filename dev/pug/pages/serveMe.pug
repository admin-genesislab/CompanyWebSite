	// $Contact section
	section#contact.contact
		.contact__inner
			h2.contact__title Contact us
			p.contact__descr We welcome your feedback, questions, comments and complaints. We keep customers at the heart of what we do, and we use your feedback to improve what we do and how we do it.
			form.contact__form(action='#', method='post')
				input#name.contact__form-input(type='text', name='name', placeholder='Your name')
				input#email.contact__form-input(type='email', name='email', placeholder='Your email')
				textarea#message.contact__form-tarea(name='message', placeholder='Your message')
				input.contact__form-submit(type='submit', value='Submit')
			.contact__card.vcard
				h3.contact__card-title Information
