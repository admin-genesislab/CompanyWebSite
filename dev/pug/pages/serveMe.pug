extends ../layouts/base

block variables
	- pageLang = ""
	- pageTitle = "Home"
	- pageDescription = "GenesisLab Desc"
	- pageKeywords = "Page-specific keywords"
	- pageUrl = ""


block body-content
	// $Page header
	header.page-header.js-header
		.page-header__inner
			.page-header__logo
				h5 Ultimate Digital Solutions
			nav.page-header__nav.js-nav
				ul
					li
						a.page-header__nav-link.js-anchor.js-link.is-active(href='#home') Home
					li
						a.page-header__nav-link.js-anchor.js-link(href='#portfolio') Portfolio
					li
						a.page-header__nav-link.js-anchor.js-link(href='#team') Our Team
					li
						a.page-header__nav-link.js-anchor.js-link(href='#contact') Contact Us


	// $Promo section
	section#home.promo.js-jumbo
		h2.promo__title
			| Serve Me<br>
		h4 Food Reservations and Ordering System.
		p.promo__descr Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt ut laoreet dolore magna aliquam erat volutpat.
		a.promo__btn(href='#') Explore more
		a.promo__go-next.js-anchor(href='#portfolio') Scroll down


	// $Portfolio section
	section#portfolio.portfolio.js-portfolio
		.portfolio__inner
			h2.portfolio__title Portfolio
			p.portfolio__descr Lorem ipsum dolor sit amet, consectetuer adipiscing elit.
			nav.portfolio__filter
				ul
					li.portfolio__filter-btn.is-selected.js-filter(data-filter='all') All
					li.portfolio__filter-btn.js-filter(data-filter='web') Web design
					li.portfolio__filter-btn.js-filter(data-filter='graphic') Graphic design
					li.portfolio__filter-btn.js-filter(data-filter='flat') Flat design
			ul.portfolio__content
				li.portfolio__item.js-item(data-cat='all flat')
					a(href='#')
						img.portfolio__item-img.js-lazy(src='static/images/port1.png', alt='')
						.portfolio__item-caption
							span.portfolio__item-title Example title
							span.portfolio__item-cat Flat design
				li.portfolio__item.js-item(data-cat='all web')
					a(href='#')
						img.portfolio__item-img.js-lazy(src='static/images/port2.png', alt='')
						.portfolio__item-caption
